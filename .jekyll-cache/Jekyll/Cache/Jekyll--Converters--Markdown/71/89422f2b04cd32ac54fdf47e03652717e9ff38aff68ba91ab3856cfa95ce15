  all of the possible values of that field.
-->

<form class="row" id="facets">
  
  <fieldset id="type-set" class="card m-1 facet-card">
    <div class="card-header facet-header">
      <a class="facet-bn" data-toggle="collapse" href="#type-collapse" role="button" aria-expanded="false" aria-controls="type-collapse">
        <legend>
          type
          <span class="facets-chevron facets-chevron-bottom float-right ml-2" aria-hidden="true"></span>
        </legend>
      </a>
      
    </div>
    <div class="collapse" id="type-collapse">
      <div class="card-body">
        
        <div class="facet-item">
          <input id="photograph" class="type" type="checkbox" name="Photograph" value="Photograph" />
          <label for="Photograph">Photograph</label>
        </div>
        
        <div class="facet-item">
          <input id="photograph" class="type" type="checkbox" name="Photograph " value="Photograph " />
          <label for="Photograph ">Photograph </label>
        </div>
        
        <div class="facet-item">
          <input id="text" class="type" type="checkbox" name="text" value="text" />
          <label for="text">text</label>
        </div>
        
        <div class="facet-item">
          <input id="text" class="type" type="checkbox" name="Text" value="Text" />
          <label for="Text">Text</label>
        </div>
        
        <div class="facet-item">
          <input id="text" class="type" type="checkbox" name="Text " value="Text " />
